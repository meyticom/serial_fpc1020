void setup() {
  /////// reset lcd ////////
  pinMode(10, OUTPUT);
  pinMode(16, INPUT_PULLUP);
  pinMode(5, INPUT_PULLUP);
  pinMode(4, INPUT_PULLUP);
  digitalWrite(10, LOW);
  delay(50);
  digitalWrite(10, HIGH);  // Turn the LED off by making the voltage HIGH
  /////// reset lcd ////////
  tft.begin();
  // initialize digital pin 16 as an output(Buzzer).
  pinMode(0, OUTPUT);
  Serial.begin(115200);    // Initialize serial communications
  delay(250);

  tft.setOrientation(1);
  tft.drawBitmap(10, 60, hotspot, 200, 48, COLOR_RED);
 
  tft.setFont(Terminal12x16);//  Terminal12x16

  tft.setOrientation(1);
  tft.drawRectangle(0, 0, tft.maxX() - 1, tft.maxY() - 1, COLOR_WHITE);

  tft.clear();
  tft.drawBitmap(5, 5, saad, 203, 24, COLOR_GREEN);
  for (uint8_t i = 0; i < 250; i++) // i<127
    tft.drawPixel(random(tft.maxX()), random(tft.maxY()), random(0xffff));
}


void loop() {
 
 
  if (digitalRead(4) == 0) { //Enter
    Serial.println("Card True");
    digitalWrite(0, HIGH);   // True
    tft.setFont(Terminal6x8);//  Terminal12x16
    delay(50);
    digitalWrite(0, LOW);    // turn the Buzzer off by making the voltage LOW
    delay(50);
    digitalWrite(0, HIGH);   // True
    delay(50);
    digitalWrite(0, LOW);    // turn the Buzzer off by making the voltage LOW
    tft.setBackgroundColor(COLOR_BLACK);
    tft.drawBitmap(10, 60, wel, 200, 48, COLOR_GREEN);
    delay(500);
    tft.drawBitmap(10, 60, wel, 200, 48, COLOR_BLACK);
  } else if (digitalRead(5)== 0) { //Exit
    Serial.println("Card True");
    digitalWrite(0, HIGH);   // True
    tft.setFont(Terminal6x8);//  Terminal12x16
    delay(50);
    digitalWrite(0, LOW);    // turn the Buzzer off by making the voltage LOW
    delay(50);
    digitalWrite(0, HIGH);   // True
    delay(50);
    digitalWrite(0, LOW);    // turn the Buzzer off by making the voltage LOW
    tft.setBackgroundColor(COLOR_BLACK);
    tft.drawBitmap(10, 60, goodbye, 200, 48, COLOR_GREEN);
    delay(500);
    tft.drawBitmap(10, 60, goodbye, 200, 48, COLOR_BLACK);
  } else if(digitalRead(16)==0) {
    digitalWrite(0, HIGH);   // True
    delay(1500);
    digitalWrite(0, LOW);    // turn the LED off by making the voltage LOW
    tft.drawBitmap(10, 60, error, 200, 48, COLOR_RED);
    delay(4000);
    tft.clear();
    for (uint8_t i = 0; i < 250; i++) // i<250
      tft.drawPixel(random(tft.maxX()), random(tft.maxY()), random(0xffff));
    tft.drawBitmap(5, 5, saad, 203, 24, COLOR_GREEN);
  }
}

